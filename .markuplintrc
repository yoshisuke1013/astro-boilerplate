{
	"parser": {
		"\\.astro$": "@markuplint/astro-parser"
	},
	"extends": [
		"markuplint:recommended"
	],
	"rules": {
		"attr-duplication": true,
		"deprecated-attr": true,
		"deprecated-element": true,
		"disallowed-element": [],
		"doctype": "always",
		"heading-levels": true,
		"id-duplication": true,
		"invalid-attr": true,
		"label-has-control": true,
		"landmark-roles": true,
		"neighbor-popovers": true,
		"no-ambiguous-navigable-target-names": true,
		"no-consecutive-br": true,
		"no-duplicate-dt": true,
		"no-empty-palpable-content": true,
		"no-orphaned-end-tag": true,
		"no-refer-to-non-existent-id": true,
		"permitted-contents": [],
		"placeholder-label-option": true,
		"require-accessible-name": true,
		"require-datetime": true,
		"required-attr": [],
		"required-element": [],
		"required-h1": true,
		"table-row-column-alignment": true,
		"use-list": true,
		"wai-aria": true
	}
}
